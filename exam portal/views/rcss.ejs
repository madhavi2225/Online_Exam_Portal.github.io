      <!-- partial -->
      <div class="main-panel">
        <div class="content-wrapper">
          <div class="row">
            <div class="col-lg-12 stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Css Result</h4>

               
                  <div class="table-responsive pt-3">
                    <table class="table table-bordered" id="myTable">
                      <thead>
                        <tr>
                          <th>Sr No.</th>
                          <th>Student Name</th>
                          <th>Email</th>
                          <th>Result</th>
                          <th>Correct</th>
                          <th>Incorrect</th>
                          <th>Date</th>
                          <th>Time</th>
                         
                        </tr>
                      </thead>
                      <tbody>
                        <% if(cssresult != null){%>
                          <% for(let i=0; i<cssresult.length; i++){%>

                              <tr>
                                  <td><%= i+1%></td>
                                  <td><%= cssresult[i].name %></td>
                                  <td><%= cssresult[i].email %></td>
                                  <td><%= cssresult[i].scocss %></td>
                                  <td><%= cssresult[i].corcss %></td>                                                    
                                  <td><%= cssresult[i].incocss %></td>                                                    
                                  <td><%= cssresult[i].css_date.toDateString() %></td>
                                  <td><%= cssresult[i].css_time.slice(0,8) %></td>
                              </tr>
                          <%}%>
                          <%  }else{%>
                              <ul>
                                  <td colspan="7">No Data Found</td>
                              </ul>
                         <% }%>

                      </tbody>
                    </table>
                  </div>
                </div>
               
              </div>
          </div>
