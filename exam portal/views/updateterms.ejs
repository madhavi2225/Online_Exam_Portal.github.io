                                <div class="card-body">
                                    <h4 class="card-title">Terms and Condition </h4>
                                    <p class="card-description">
                                        Adding Table
                                    </p>
                                    <!-- Modal HTML embedded directly into document -->

                                    <a class="btn" href="#ex2" rel="modal:open"><button
                                            class="btn btn-primary rounded-sm" style="margin-left: 970px;">ADD
                                            TERMS</button></a>

                                    <!-- Modal HTML embedded directly into document -->
                                    <form action="/updateterms/create" method="post" class="login_form modal" id="ex2"
                                        style="display:none; height: 350px;">
                                        <h3>
                                            <center>please enter terms
                                                <center>
                                        </h3>
                                        <br>
                                        <p align="middle"><label>Terms: &nbsp&nbsp&nbsp </label><input type="text"
                                                name="terms" id="terms" /></p>
                                        <h6 align="middle" id="errort"></h6>
                                        <br>
                                        <p align="middle"><input type="submit" id="add" value="Add Terms"
                                                class="btn btn-primary rounded-sm" style="text-transform: uppercase" />
                                        </p>
                                        <p align="middle">
                                            <!-- <button type="reset">Reset</button> -->
                                            <input type="reset" value="Reset User"
                                                class="btn btn-primary rounded-sm"
                                                style="text-transform: uppercase" />
                                        </p>
                                    </form>



                                    <div class="table-responsive pt-3">
                                        <table class="table table-bordered" id="myTable">
                                            <script> scrollX= true</script>
                                            <!-- <thead>
                                                <tr>
                                                    <th>
                                                        SR.No
                                                    </th>
                                                    <th>
                                                        terms and condition
                                                    </th>

                                                    <th>
                                                        Update
                                                    </th>
                                                    <th>
                                                        Delete
                                                    </th>
                                                </tr>
                                            </thead> -->
                                            <tbody>
                                                <% if(userdata.length!=0){ var i=1; userdata.forEach(function(data){ %>

                                                    <tr class="table-info">
                                                        <td>
                                                            <%=i ; %>
                                                        </td>
                                                        <td>
                                                            <%= data.terms %>
                                                        </td>

                                                        <!-- <td>
                                                            <button class="btn btn-success rounded-sm">UPDATE</button>
                                                        </td> -->
                                                        <td>
                                                            <a class="btn" href="#ex3<%= data.id %>" rel="modal:open">
                                                                <button class="btn btn-success rounded-sm"
                                                                    style="float: left;">Update</button>
                                                            </a>

                                                            <form action="/upterms/<%= data.id %>" method="post"
                                                                class="login_form modal" id="ex3<%= data.id %>"
                                                                style="display:none; height: 350px;">
                                                                <h3>
                                                                    <center>please enter terms
                                                                        <center>
                                                                </h3>
                                                                <br>
                                                                <p align="middle"><label>Terms: &nbsp&nbsp&nbsp
                                                                    </label><input type="text" name="terms"
                                                                        id="upterms<%= data.id %>"
                                                                        value="<%= data.terms %>" /></p>
                                                                <h6 align="middle" id="uperrort<%= data.id %>"></h6>
                                                                <br>
                                                                <p align="middle"><input type="submit"
                                                                        id="upadd<%= data.id %>" value="Update"
                                                                        onclick="return update('upterms<%= data.id %>','uperrort<%= data.id %>')"
                                                                        class="btn btn-success rounded-sm"
                                                                        style="text-transform: uppercase" /></p>
                                                                        <p align="middle">
                                                                            <!-- <button type="reset">Reset</button> -->
                                                                            <input type="reset" value="Reset User"
                                                                                class="btn btn-primary rounded-sm"
                                                                                style="text-transform: uppercase" />
                                                                        </p>
                                                            </form>
                                                        </td>
                                                        <!-- <td>
                                                            <button class="btn btn-danger rounded-sm">DELETE</button>
                                                        </td> -->
                                                        <td>
                                                            <!-- <button class="btn btn-danger rounded-sm">DELETE</button> -->
                                                            <a href="/deltndc/<%= data.terms %>"
                                                                class="btn btn-danger rounded-sm">DELETE</a>
                                                        </td>
                                                    </tr>

                                                    <% i++; }) %>
                                                        <% } else{ %>
                                                            <ul>
                                                                <td colspan="7">No Data Found</td>
                                                            </ul>
                                                            <% } %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <script>
                                    $(() => {
                                        var flag = true;
                                        // Validate name
                                        $('#errort').css("color", "red")
                                        //  let nameError = true;
                                        $('#terms').keyup(function () {
                                            validatet();
                                        });

                                        function validatet() {
                                            let nameValue = $('#terms').val();
                                            if (nameValue.length == '') {
                                                $('#errort').show();
                                                $('#errort').html("enter term");
                                                flag = false;
                                                // nameError = false;
                                                // return false;
                                            } else if (nameValue.length < 2) {
                                                $('#errort').show();
                                                $('#errort').html("term must have atleast 2 letters.");
                                                flag = false;
                                                // nameError = false;
                                                // return false;
                                            } else {
                                                $('#errort').hide();
                                                flag = true;
                                            }
                                        }
                                        $("#add").click(() => {
                                            validatet();

                                            return flag;

                                        });
                                    });

                                </script>
                                <script>
                                    function update(xyz, www) {
                                        // console.log("hamojha");
                                        console.log(document.getElementById(`${xyz}`).value);
                                        if (document.getElementById(`${xyz}`).value == '') {
                                            // console.log("plz enter value");
                                            document.getElementById(`${www}`).innerHTML = "Plzz Enter value";
                                            return false;
                                        }
                                        else {
                                            document.getElementById(`${www}`).innerHTML = "";
                                        }

                                    }
                                </script>
                            </div>
                        </div>
